<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Doze: does this device support STATE_DOZE?  -->
    <bool name="doze_display_state_supported">true</bool>

    <!-- Set audio panel to the left side -->
    <bool name="config_audioPanelOnLeftSide">true</bool>

    <item name="pixel_pitch" format="float" type="dimen">61.2</item>

    <color name="config_udfpsColor">#24dcff</color>

    <!-- If not zero, limits the internal screen recorder's framerate to the set value. -->
    <integer name="config_screenRecorderMaxFramerate">60</integer>

    <!-- If not empty, sets the AVC codec profile level used for the internal screen recorder.
         Possible values: 3, 3.1, 3.2, 4, 4.1, 4.2
    -->
    <string name="config_screenRecorderAVCProfileLevel" translatable="false">3</string>
</resources>
